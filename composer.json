{
    "name": "frankiejarrett/fluentllm",
    "description": "A fluent interface for calling LLM providers in Laravel",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Frankie Jarrett",
            "email": "frankie@example.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "guzzlehttp/guzzle": "^7.8",
        "illuminate/contracts": "^10.0|^11.0",
        "illuminate/support": "^10.0|^11.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.1",
        "mockery/mockery": "^1.5",
        "orchestra/testbench": "^8.0|^9.0",
        "pestphp/pest": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "FluentLLM\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "FluentLLM\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "FluentLLM\\FluentLLMServiceProvider"
            ],
            "aliases": {
                "LLM": "FluentLLM\\Facades\\LLM"
            }
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage"
    }
}
